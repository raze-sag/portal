{
    "useTabs": false, 
    "tabWidth": 2,
    "singleQuote": true,
    "jsxSingleQuote": true, 
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "printWidth": 80, 
    "proseWrap": "always",
    "overrides": [
        {
            "files": "*.tsx",
            "options": {
                "parser": "typescript"
            }
        }
    ]
}
